{
  "default_context": {
    "uri": "http://www.bindingdb.org/ontology/",
    "prefix": "bindingdb"
  },
  "source_plan": {
    "context": {
      "uri": "https://pubchem.ncbi.nlm.nih.gov/compound/",
      "prefix": "cid"
    },
    "id_column": "PubChem CID",
    "node_name_column": "BindingDB Ligand Name",
    "property_columns": [
      "BindingDB MonomerID",
      "PubChem SID",
      "ChEBI ID of Ligand",
      "ChEMBL ID of Ligand",
      "DrugBank ID of Ligand",
      "IUPHAR_GRAC ID of Ligand",
      "KEGG ID of Ligand",
      "ZINC ID of Ligand",
      "Ligand SMILES",
      "Ligand InChI",
      "Ligand InChI Key",
      "BindingDB Ligand Name",
      "Link to Ligand in BindingDB"
    ]
  },
  "target_plan": {
    "context": {
      "uri": "http://identifiers.org/hgnc.symbol/",
      "prefix": "HGNC"
    },
    "id_column": "HGNC Symbol",
    "property_columns": [
      "Number of Protein Chains in Target",
      "BindingDB Target Chain  Sequence",
      "PDB ID(s) of Target Chain",
      "UniProt (SwissProt) Recommended Name of Target Chain",
      "Target Name Assigned by Curator or DataSource",
      "UniProt (SwissProt) Secondary ID(s) of Target Chain",
      "UniProt (SwissProt) Alternative ID(s) of Target Chain",
      "UniProt (TrEMBL) Submitted Name of Target Chain",
      "UniProt (TrEMBL) Entry Name of Target Chain",
      "UniProt (TrEMBL) Primary ID of Target Chain",
      "UniProt (TrEMBL) Secondary ID(s) of Target Chain",
      "UniProt (TrEMBL) Alternative ID(s) of Target Chain",
      "Target Source Organism According to Curator or DataSource",
      "UniProt (SwissProt) Primary ID of Target Chain",
      "Link to Target in BindingDB"
    ]
  },
  "edge_plan": {
    "default_predicate": "binds",
    "predicate_id_column": null,
    "predicate_context": null,
    "id_column": null,
    "property_columns": [
      "Ki (nM)",
      "IC50 (nM)",
      "Kd (nM)",
      "EC50 (nM)",
      "kon (M-1-s-1)",
      "koff (s-1)",
      "pH",
      "Temp (C)",
      "Curation/DataSource",
      "Article DOI",
      "PMID",
      "PubChem AID",
      "Patent Number",
      "Institution",
      "Link to Ligand-Target Pair in BindingDB",
      "Ligand HET ID in PDB",
      "PDB ID(s) for Ligand-Target Complex"
