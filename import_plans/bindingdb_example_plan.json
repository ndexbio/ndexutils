    "citation_plan": {
      "citation_id_columns": [
        {
          "id": "PMID",
          "type": "pmid",

        },
        {
          "id": "Article DOI",
          "type": "DOI"
        }
      ],
      "contributors_column": "Authors"
    }
  }
